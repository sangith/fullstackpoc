{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:pact": "jest --testMatch \"**/*.pact.test.ts\"",
    "storybook": "ng run frontend:storybook",
    "build-storybook": "ng run frontend:build-storybook",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "msw:init": "npx msw init public --save"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/router": "^19.2.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.9",
    "@angular/language-service": "^19.2.9",
    "@pact-foundation/pact": "^10.0.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/angular": "^8.6.12",
    "@storybook/builder-webpack5": "^8.6.12",
    "@storybook/jest": "^0.2.2",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "^0.2.2",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "chromatic": "^11.28.2",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "msw": "^2.7.5",
    "msw-storybook-addon": "^2.0.4",
    "ts-jest": "^29.3.2",
    "typescript": "~5.7.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}